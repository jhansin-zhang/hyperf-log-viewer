{
    "name": "giikin/hyperf-log-viewer",
    "description": "A log viewer component for Hyperf framework with aggregation support",
    "license": "MIT",
    "type": "library",
    "keywords": ["hyperf", "log", "viewer"],
    "require": {
        "php": ">=8.1",
        "hyperf/framework": "~3.1.0",
        "hyperf/http-server": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Giikin\\HyperfLogViewer\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Giikin\\HyperfLogViewer\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    }
}
